{
  "name": "react-document-timeline",
  "version": "1.0.0",
  "description": "react document timeline",
  "private": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://silver-it@github.com/hololux/react-document-timeline"
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "start": "react-scripts start",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn build:css && cp src/type.ts lib/",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:css": "node-sass src/scss/style.scss lib/style.css",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,demo/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "yarn test && yarn run prettier && yarn run lint"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "document",
    "timeline"
  ],
  "author": "Hololux",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/node": "^18.11.10",
    "@types/react-dom": "^18.0.9",
    "enzyme": "^3.11.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.2",
    "node-sass": "^8.0.0",
    "react-i18next": "^12.0.0",
    "tslib": "^2.4.1"
  }
}